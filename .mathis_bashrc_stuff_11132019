# Add my bash aliases from .bash-aliases
# any PATH modifications are in .profile
if [ -f ~/.bash_aliases ]; then
      source ~/.bash_aliases
fi

#easy way to view md files in lynx
readmd () {
    pandoc $1 | lynx -stdin
}

#memeteamdreams
hellothere () {
  echo "general kenobi"
}
